datasource db {
  provider = "mongodb"
  url      = "mongodb+srv://git:food@cluster0.k3umsur.mongodb.net/test"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model Food {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String?
  price       String?
  ingredients String?
  active      String?
  description String?
  imageUrl    String?
}
